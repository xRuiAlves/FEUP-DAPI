    <!-- http://www.semanticweb.org/rui/ontologies/2020/11/goodreads#357473 -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/rui/ontologies/2020/11/goodreads#357473">
        <rdf:type rdf:resource="http://www.semanticweb.org/rui/ontologies/2020/11/goodreads#Review"/>
        <reviews rdf:resource="http://www.semanticweb.org/rui/ontologies/2020/11/goodreads#PrideAndPrejudice"/>
        <wasPostedOn rdf:resource="http://www.semanticweb.org/rui/ontologies/2020/11/goodreads#2017-01-12"/>
        <hasContent rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Love, love, loved it!!! The ultimate regency romance!</hasContent>
        <hasRating rdf:datatype="http://www.w3.org/2001/XMLSchema#float">5.0</hasRating>
        <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2017-01-12</dc:date>
        <dc:identifier rdf:datatype="http://www.w3.org/2001/XMLSchema#string">357473</dc:identifier>
